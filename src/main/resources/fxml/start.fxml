<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="@biztonsag.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Biztonsag">
   <children>
      <Button fx:id="btnLNKO" layoutX="51.0" layoutY="49.0" mnemonicParsing="false" onAction="#lnko" prefHeight="49.0" prefWidth="296.0" text="LNKO számítás">
         <font>
            <Font name="Calibri" size="14.0" />
         </font></Button>
      <Button fx:id="btnBezar" alignment="CENTER" cancelButton="true" contentDisplay="RIGHT" layoutX="240.0" layoutY="520.0" mnemonicParsing="false" onAction="#bezar" prefHeight="49.0" prefWidth="382.0" text="Program befejezése">
         <padding>
            <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
         </padding>
         <font>
            <Font name="Calibri" size="14.0" />
         </font></Button>
      <Button fx:id="btnCalc" layoutX="474.0" layoutY="49.0" mnemonicParsing="false" onAction="#calculator" prefHeight="49.0" prefWidth="296.0" text="Számológép">
         <font>
            <Font name="Calibri" size="14.0" />
         </font></Button>
      <Button fx:id="btnMiller" layoutX="51.0" layoutY="117.0" mnemonicParsing="false" onAction="#miller" prefHeight="49.0" prefWidth="296.0" text="Miller-Rabin teszt">
         <font>
            <Font name="Calibri" size="14.0" />
         </font>
      </Button>
      <ImageView fitHeight="299.0" fitWidth="296.0" layoutX="278.0" layoutY="199.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@logo.png" />
         </image>
      </ImageView>
      <Button fx:id="btnLista" layoutX="474.0" layoutY="117.0" mnemonicParsing="false" onAction="#lista" prefHeight="49.0" prefWidth="296.0" text="Adatok listázása">
         <font>
            <Font name="Calibri" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
